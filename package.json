{
  "name": "transpile-md-to-json",
  "version": "0.1.3",
  "description": "Transpile multiple markdown files to a single JSON file.",
  "engines": {
    "node": ">=10.17.0"
  },
  "bin": {
    "transpile-md-to-json": "./bin/transpile-md-to-json.js"
  },
  "main": "build/index.js",
  "dependencies": {
    "@sindresorhus/slugify": "^0.11.0",
    "camelcase": "^5.3.1",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.1",
    "commander": "^4.1.1",
    "dot-prop": "^5.2.0",
    "flat": "^5.0.0",
    "readdirp": "^3.3.0"
  },
  "scripts": {
    "start": "node build/index.js",
    "code": "tsc -w",
    "build": "rm -fr build/*; tsc",
    "ncu": "ncu -u"
  },
  "author": "Sun Knudsen <hello@sunknudsen.com>",
  "license": "MIT",
  "keywords": [
    "cli",
    "transpiler",
    "markdown",
    "markdown-to-jsx",
    "json",
    "react"
  ],
  "devDependencies": {
    "@types/flat": "0.0.28",
    "@types/node": "^13.7.4",
    "npm-check-updates": "^4.0.1",
    "typescript": "^3.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sunknudsen/transpile-md-to-json.git"
  },
  "bugs": {
    "url": "https://github.com/sunknudsen/transpile-md-to-json/issues"
  },
  "homepage": "https://github.com/sunknudsen/transpile-md-to-json#readme",
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
