{
  "name": "transpile-md-to-json",
  "version": "0.2.1",
  "description": "Transpile multiple markdown files to a single JSON file.",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "type": "module",
  "bin": {
    "transpile-md-to-json": "./bin/transpile-md-to-json.js"
  },
  "exports": "./build/index.js",
  "files": [
    "bin/",
    "build/",
    "package.json",
    "LICENCE",
    "README.md"
  ],
  "dependencies": {
    "@sindresorhus/slugify": "^2.1.0",
    "camelcase": "^6.3.0",
    "chalk": "^5.0.1",
    "chokidar": "^3.5.3",
    "commander": "^9.2.0",
    "dot-prop": "^7.2.0",
    "escape-string-regexp": "^5.0.0",
    "execa": "^6.1.0",
    "flat": "^5.0.2",
    "fs-extra": "^10.1.0",
    "readdirp": "^3.6.0"
  },
  "scripts": {
    "code": "tsc -w",
    "build": "rm -fr build/*; tsc",
    "prepublishOnly": "npm run build",
    "ncu": "ncu -u"
  },
  "author": "Sun Knudsen <hello@sunknudsen.com>",
  "license": "MIT",
  "keywords": [
    "cli",
    "transpiler",
    "markdown",
    "markdown-to-jsx",
    "json",
    "react"
  ],
  "devDependencies": {
    "@types/flat": "5.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.31",
    "npm-check-updates": "^12.5.11",
    "typescript": "^4.6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sunknudsen/transpile-md-to-json.git"
  },
  "bugs": {
    "url": "https://github.com/sunknudsen/transpile-md-to-json/issues"
  },
  "homepage": "https://github.com/sunknudsen/transpile-md-to-json#readme",
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
