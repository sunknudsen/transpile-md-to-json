{
  "name": "transpile-md-to-json",
  "version": "0.3.2",
  "description": "Transpile multiple markdown files to a single JSON file.",
  "engines": {
    "node": ">=14.16"
  },
  "type": "module",
  "bin": {
    "transpile-md-to-json": "./bin/transpile-md-to-json.js"
  },
  "exports": "./build/index.js",
  "files": [
    "bin/",
    "build/",
    "package.json",
    "LICENCE",
    "README.md"
  ],
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1",
    "camelcase": "^8.0.0",
    "chalk": "^5.4.1",
    "chokidar": "^4.0.3",
    "commander": "^13.1.0",
    "dot-prop": "^9.0.0",
    "escape-string-regexp": "^5.0.0",
    "execa": "^9.5.3",
    "flat": "^6.0.1",
    "fs-extra": "^11.3.0",
    "readdirp": "^4.1.2"
  },
  "scripts": {
    "code": "tsc -w",
    "build": "rm -fr build/*; tsc",
    "prepublishOnly": "npm run build",
    "ncu": "ncu --target minor --upgrade"
  },
  "author": "Sun Knudsen <hello@sunknudsen.com>",
  "license": "MIT",
  "keywords": [
    "cli",
    "transpiler",
    "markdown",
    "markdown-to-jsx",
    "json",
    "react"
  ],
  "devDependencies": {
    "@types/flat": "5.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.17",
    "npm-check-updates": "^18.0.1",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sunknudsen/transpile-md-to-json.git"
  },
  "bugs": {
    "url": "https://github.com/sunknudsen/transpile-md-to-json/issues"
  },
  "homepage": "https://github.com/sunknudsen/transpile-md-to-json#readme",
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
