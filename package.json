{
  "name": "transpile-md-to-json",
  "version": "0.1.15",
  "description": "Transpile multiple markdown files to a single JSON file.",
  "engines": {
    "node": ">=10.17.0"
  },
  "bin": {
    "transpile-md-to-json": "./bin/transpile-md-to-json.js"
  },
  "main": "build/index.js",
  "dependencies": {
    "@sindresorhus/slugify": "^1.1.0",
    "camelcase": "^6.2.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.5.1",
    "commander": "^7.0.0",
    "dot-prop": "^6.0.1",
    "escape-string-regexp": "^4.0.0",
    "flat": "^5.0.2",
    "fs-extra": "^9.1.0",
    "readdirp": "^3.5.0"
  },
  "scripts": {
    "code": "tsc -w",
    "build": "rm -fr build/*; tsc",
    "prepublishOnly": "npm run build",
    "ncu": "ncu -u"
  },
  "author": "Sun Knudsen <hello@sunknudsen.com>",
  "license": "MIT",
  "keywords": [
    "cli",
    "transpiler",
    "markdown",
    "markdown-to-jsx",
    "json",
    "react"
  ],
  "devDependencies": {
    "@types/flat": "5.0.1",
    "@types/fs-extra": "^9.0.7",
    "@types/node": "^14.14.27",
    "npm-check-updates": "^11.1.1",
    "typescript": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sunknudsen/transpile-md-to-json.git"
  },
  "bugs": {
    "url": "https://github.com/sunknudsen/transpile-md-to-json/issues"
  },
  "homepage": "https://github.com/sunknudsen/transpile-md-to-json#readme",
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
